  <%= vue_component 'LandingPage',
      {
        left:   [  { label: 'StudyApp', url: root_path },
                    { label: 'About',      url: "#about"  }],
        center: [
          { label: 'Dashboard', url: "#dashboard" },
          { label: 'Workspace', url: "#workspace" }
        ],
        right:  if user_signed_in?
                  [
                    { label: 'Dashboard', url: dashboard_path, variant: 'gradient' },
                    { label: 'Deconexion', url: logout_path, variant: 'outline', data: { 'data-turbo-method': :delete } }
